!!import:"strings"
<!DOCTYPE html>
<html>
    <head>
        <title>Example</title>
    </head>
    <body>
        <ul>
            <| posts := []string{ "Foo", "Bar", "Baz" } |><| for _, post := range posts { |>
                <li><|:w strings.ToUpper(post) |></li>
            <| } |>
        </ul>
    </body>
</html>
